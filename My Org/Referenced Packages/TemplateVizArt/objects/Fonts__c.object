<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Used to define available fonts in the WYSIWYG editor. Each entry defines one item in the font drop-down list. Use the name field as drop-down list label, the font list to provide a list of fonts that are injected in the HTML as font-family.</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Is_Active__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Used to deactivate single fonts. Please use this for standard fonts if you don&apos;t want to display them.</inlineHelpText>
        <label>Is Active</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Value__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>List fonts separated by comma which get injected for selected font in drop-down.</inlineHelpText>
        <label>Font List</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <label>Font</label>
</CustomObject>
